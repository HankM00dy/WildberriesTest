<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="RunOnlyThirdTests">
    <test name="RunOnlyThirdTests">
        <groups>
            <run>
                <exclude name="ThirdTest"></exclude>
            </run>
            <dependencies>
                <group name="NegativeTests" depends-on="PositiveTests"/>
            </dependencies>
        </groups>
        <classes>
            <class name="frontend.suites.SmokeTest"></class>
            <class name="frontend.suites.RegressTest"></class>
            <class name="frontend.suites.FailureTest"></class>
        </classes>
    </test>
</suite>

